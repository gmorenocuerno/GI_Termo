<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="InfinityGComissionsPU" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>java:/comtermoapp</jta-data-source>
        <class>com.gpinfinity.entities.Usuarios</class>
        <class>com.gpinfinity.entities.TerAreaNegocio</class>
        <class>com.gpinfinity.entities.TerEmpleado</class>
        <class>com.gpinfinity.entities.TerEmpleadoFamilia</class>
        <class>com.gpinfinity.entities.TerEmpleadoFamiliaIndicador</class>
        <class>com.gpinfinity.entities.TerFamilia</class>
        <class>com.gpinfinity.entities.TerParametroIndicador</class>
        <class>com.gpinfinity.entities.TerIndicadorAreaNegocio</class>
        <class>com.gpinfinity.entities.TerComisionesTextil</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties>
            <property name="javax.persistence.schema-generation.database.action" value="create"/>
            <property name="jboss.as.jpa.managed" value="false"/>
        </properties>
    </persistence-unit>
</persistence>
